{
  "name": "thelonius/framework",
  "type": "library",
  "description": "A micro-framework for WordPress",
  "keywords": [ "wp", "wordpress", "thelonius", "framework" ],
  "license": "MIT",
  "authors": [
    {
      "name": "Chauncey McAskill",
      "email": "chauncey@mcaskill.ca"
    }
  ],
  "support": {
    "issues": "https://github.com/mcaskill/thelonius/issues",
    "docs": "https://github.com/mcaskill/thelonius/wiki"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.5",
    "container-interop/container-interop": "^1.0",
    "hassankhan/config": "^0.10",
    "illuminate/support": "^5.2",
    "pimple/pimple": "^3.0",
    "vlucas/phpdotenv": "^2.1"
  },
  "require-dev": {
    "phpdocumentor/phpdocumentor": "^2.0"
  },
  "suggest": {
    "johnpbloch/wordpress": "The Composer version of WordPress."
  },
  "autoload": {
    "psr-4": {
      "Thelonius\\": "src/"
    },
    "files": [
      "src/helpers.php",
      "src/events.php"
    ]
  }
}
